{
    "name": "Scrapeit Chrome Extension",
    "description": "Scrapeit",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./favicon.png",
        "32": "./favicon.png",
        "70": "./favicon.png",
        "144": "./favicon.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ] 
}